  "scripts": {
    "dev": "ts-node-dev --no-notify src/server.ts",
    "build": "tsc",
    "postbuild": "cpy '**/*' '!**/*.ts' ../build/ --cwd=src/ --no-overwrite --parents",
    "start": "node build/src/server.js",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint-fix": "eslint --fix src/ tests/ --ext .ts",
    "api-check": "swagger-cli validate openapi.yml",
    "doc-to-json": "swagger-cli bundle --outfile openapi.json openapi.yml",
    "postinstall": "prisma generate",
    "test": "NODE_ENV=test nyc mocha -r ts-node/register tests/**/*.test.ts --exit"
