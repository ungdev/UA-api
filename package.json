  "scripts": {
    "dev": "ts-node-dev --no-notify --exit-child src/server.ts",
    "build": "tsc",
    "postbuild": "cpy '**/*' '!**/*.ts' ../build/ --cwd=src/ --no-overwrite --parents",
    "start": "node build/src/server.js",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:fix": "eslint --fix src/ tests/ --ext .ts",
    "swagger:build": "swagger-cli bundle --outfile openapi.json openapi.yml",
    "swagger:check": "swagger-cli validate openapi.yml",
    "postinstall": "prisma generate",
    "test": "mocha tests/**/*.test.ts",
    "test:coverage": "nyc yarn test",
    "discord:sync:dev": "ts-node-dev --no-notify src/scripts/discordSync.ts",
    "discord:sync:start": "node build/src/scripts/discordSync.js",
    "discord:reset:dev": "ts-node-dev --no-notify src/scripts/discordReset.ts",
